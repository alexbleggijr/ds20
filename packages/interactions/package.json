{
  "name": "@templarios/interactions",
  "version": "0.0.1",
  "description": "Templ√°rios TypeScript library focusing in animations and interactions using Ionic Animations, Ionic Gestures and GSAP.",
  "private": true,
  "type": "module",
  "main": "./interactions.js",
  "module": "./interactions.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./interactions.js"
    }
  },
  "scripts": {
    "storybook:start": "storybook dev -p 6006",
